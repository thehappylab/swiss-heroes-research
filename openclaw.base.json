{
  "commands": {
    "restart": true
  },
  "audio": {
    "transcribe": {
      "enabled": true,
      "provider": "openai-whisper"
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "openrouter/moonshotai/kimi-k2.5"
      },
      "workspace": "/data/openclaw-data/workspace-operation",
      "models": {
        "openrouter/auto": {
          "alias": "OpenRouter"
        },
        "openrouter/minimax/minimax-m2.5": {},
        "openrouter/anthropic/claude-opus-4.6": {},
        "openrouter/openai/gpt-5.2-codex": {}
      },
      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    },
    "list": [
      {
        "id": "operation-bot",
        "name": "Tony",
        "workspace": "/data/openclaw-data/workspace-growth",
        "model": "openrouter/moonshotai/kimi-k2.5"
      },
      {
        "id": "builder-bot",
        "name": "Bob",
        "workspace": "/data/openclaw-data/workspace-build",
        "model": "openrouter/moonshotai/kimi-k2.5"
      },
      {
        "id": "operation-bot",
        "name": "Nici",
        "default": true,
        "workspace": "/data/openclaw-data/workspace-operation",
        "model": "openrouter/moonshotai/kimi-k2.5"
      }
    ]
  },
  "tools": {
    "agentToAgent": {
      "enabled": true,
      "allow": ["growth-bot", "builder-bot", "operation-bot"]
    },
    "web": {
      "search": {
        "enabled": true,
        "provider": "brave",
        "apiKey": "${BRAVE_WEBSEARCH_API_KEY}"
      }
    }
  },
  "channels": {
    "discord": {
      "enabled": true,
      "groupPolicy": "allowlist",
      "defaultAccount": "operation-bot",
      "accounts": {
        "growth-bot": {
          "groupPolicy": "open",
          "token": "${DISCORD_GROWTH_BOT_TOKEN}"
        },
        "builder-bot": {
          "groupPolicy": "open",
          "token": "${DISCORD_BUILDER_BOT_TOKEN}"
        },
        "operation-bot": {
          "groupPolicy": "open",
          "token": "${DISCORD_OPERATION_BOT_TOKEN}"
        }
      }
    }
  },
  "bindings": [
    {
      "agentId": "growth-bot",
      "match": {
        "channel": "discord",
        "accountId": "growth-bot"
      }
    },
    {
      "agentId": "builder-bot",
      "match": {
        "channel": "discord",
        "accountId": "builder-bot"
      }
    },
    {
      "agentId": "operation-bot",
      "match": {
        "channel": "discord",
        "accountId": "operation-bot"
      }
    }
  ],
  "skills": {
    "load": {
      "extraDirs": [
        "/data/openclaw-data/skills"
      ]
    }
  }
}
