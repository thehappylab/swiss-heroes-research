{
  "agents": {
    "defaults": {
      "model": {
        "primary": "openrouter/moonshotai/kimi-k2.5"
      },
      "models": {
        "openrouter/auto": {
          "alias": "OpenRouter"
        },
        "openrouter/anthropic/claude-opus-4.6": {},
        "openrouter/moonshotai/kimi-k2.5": {},
        "openrouter/openai/gpt-5.2-codex": {}
      },
      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    },
    "list": [
      {
        "id": "growth",
        "name": "Tony",
        "default": true,
        "workspace": "/data/openclaw-data/workspace-growth",
        "model": "openrouter/moonshotai/kimi-k2.5"
      },
      {
        "id": "builder",
        "name": "Bob",
        "workspace": "/data/openclaw-data/workspace-build",
        "model": "openrouter/anthropic/claude-opus-4.6"
      },
      {
        "id": "operation",
        "name": "Nici",
        "workspace": "/data/openclaw-data/workspace-operation",
        "model": "openrouter/moonshotai/kimi-k2.5"
      }
    ]
  },
  "tools": {
    "agentToAgent": {
      "enabled": true,
      "allow": ["growth", "builder", "operation"]
    },
    "web": {
      "search": {
        "enabled": true,
        "provider": "brave",
        "apiKey": "${BRAVE_WEBSEARCH_API_KEY}"
      }
    }
  },
  "channels": {
    "discord": {
      "enabled": true,
      "accounts": {
        "growth-bot": {
          "groupPolicy": "open",
          "token": "${DISCORD_GROWTH_BOT_TOKEN}"
        },
        "builder-bot": {
          "groupPolicy": "open",
          "token": "${DISCORD_BUILDER_BOT_TOKEN}"
        },
        "operation-bot": {
          "groupPolicy": "open",
          "token": "${DISCORD_OPERATION_BOT_TOKEN}"
        }
      }
    }
  },
  "bindings": [
    {
      "agentId": "growth",
      "match": {
        "channel": "discord",
        "accountId": "growth-bot"
      }
    },
    {
      "agentId": "builder",
      "match": {
        "channel": "discord",
        "accountId": "builder-bot"
      }
    },
    {
      "agentId": "operation",
      "match": {
        "channel": "discord",
        "accountId": "operation-bot"
      }
    }
  ],
  "skills": {
    "load": {
      "extraDirs": [
        "/data/openclaw-data/skills",
        "/data/workspace/skills"
      ]
    },
    "entries": {
      "video-transcript-downloader": { "enabled": true },
      "frontend-design": { "enabled": true },
      "create-cli": { "enabled": true },
      "summarize": { "enabled": true },
      "obsidian": { "enabled": true },
      "bitwarden-vault": { "enabled": true }
    }
  }
}
